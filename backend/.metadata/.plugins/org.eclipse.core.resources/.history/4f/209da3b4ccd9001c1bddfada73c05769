package com.beater.vehicle.model;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

import com.sun.istack.NotNull;

@Entity
@Table(name = "Vehicle")
public class Vehicle {
	@Id
    @Column(name = "vehicle_id")
    @SequenceGenerator(initialValue=1000,allocationSize=1,name = "vehicle_sequence", sequenceName="vehicle_sequence")
    @GeneratedValue(generator="vehicle_sequence")
	private long vehicleId;
	@Column(name = "vehicle_make")
	private String make;
	@Column(name= "vehicle_Model")
	private String model;
	@Column(name="variant")
	private String variant;
	@Column(name = "dateOfRegister")
    private Date dateOfRegister;
	@Column(name="vin_num")
	@NotNull
	private long vinNumber;
	@Column(name = "reg_num")
	@NotNull
	private long regNumber;
	@Column(name = "color")
	private String color;
	

}
